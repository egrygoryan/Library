@page
@model LibraryApp.Pages.BookModel
@{
	ViewData["Title"] = "BooksList";
}
<h1>Books</h1>
<table class="table mt-5">
	<thead>
		<tr>
			<th scope="col">Title</th>
			<th scope="col">Year</th>
		</tr>
	</thead>
	@foreach (var book in Model.Books) {
		<tr>
			<td>@book.Title</td>
			<td>@book.Year</td>
		</tr>
	}
</table>

